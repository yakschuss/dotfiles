if &compatible
  set nocompatible
end

" Dracula colorscheme
call minpac#add('dracula/vim')

" Run native commands
call minpac#add('christoomey/vim-run-interactive')

" Integrate vim with tmux
call minpac#add('christoomey/vim-tmux-navigator')

" Test runner integration
call minpac#add('vim-test/vim-test')

" Edit files that don't exist by creating files
call minpac#add('pbrisbin/vim-mkdir')

" Bundler integration
call minpac#add('tpope/vim-bundler')

" Map cp to pbcopy
call minpac#add('christoomey/vim-system-copy')

" FZF integration
call minpac#add('junegunn/fzf', { 'dir': '~/.fzf', 'do': './install --all' })
call minpac#add('junegunn/fzf.vim')

" Ripgrep plugin
call minpac#add('jremmen/vim-ripgrep')

" Automatic ending for certain code structures
call minpac#add('tpope/vim-endwise')

" UNIX command integration
call minpac#add('tpope/vim-eunuch')

" Git wrapper
call minpac#add('tpope/vim-fugitive')
call minpac#add('christoomey/vim-conflicted')

" Gem configuration
call minpac#add('tpope/vim-projectionist')

" Rails integration
call minpac#add('tpope/vim-rails')

" Vim-rails like support for non rails projects
call minpac#add('tpope/vim-rake')

" Allow plugins to repeat using . command
call minpac#add('tpope/vim-repeat')

" Interface with hub commands in git commit editor
call minpac#add('tpope/vim-rhubarb')

" Use surround commands to surround text objects
call minpac#add('tpope/vim-surround')

" Comment out text objects
call minpac#add('vim-scripts/tComment')

" Syntax highlighting for different languages
call minpac#add('sheerun/vim-polyglot')

" Emmet for vim
call minpac#add('mattn/emmet-vim')

" Automatically close HTML tags
call minpac#add('alvan/vim-closetag')

" Show git changes in sidebar
call minpac#add('airblade/vim-gitgutter')

" Search and replace using quickfix
call minpac#add('vim-scripts/greplace.vim')

" Textobjects - ruby
call minpac#add('nelstrom/vim-textobj-rubyblock')
call minpac#add('kana/vim-textobj-user')

" Mappings
call minpac#add('tpope/vim-unimpaired')

" Colorscheme for Gdiff
call minpac#add('endel/vim-github-colorscheme')

" Vim-tmux-runner
call minpac#add('christoomey/vim-tmux-runner')

" Align stuff
call minpac#add('junegunn/vim-easy-align')

" Rubocop integration
call minpac#add('ngmy/vim-rubocop')

" LLM Configs
call minpac#add('nvim-lua/plenary.nvim')
call minpac#add('OXY2DEV/markview.nvim')
